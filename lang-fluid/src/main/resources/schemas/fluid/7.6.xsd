                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute type="xsd:string" name="class" use="optional"
                           default="NULL">
                <xsd:annotation>
                    <xsd:documentation>
                        <![CDATA[CSS class(es) for this element]]>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute type="xsd:string" name="dir" use="optional"
                           default="NULL">
                <xsd:annotation>
                    <xsd:documentation>
