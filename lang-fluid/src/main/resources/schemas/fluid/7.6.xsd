                    <xsd:documentation>
                        <![CDATA[list of GET variables to store. By default the current id, module and all module arguments]]>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute type="xsd:array" name="setVars" use="optional"
                           default="array ( )">
                <xsd:annotation>
                    <xsd:documentation>
                        <![CDATA[list of SET[] variables to store. See]]>
