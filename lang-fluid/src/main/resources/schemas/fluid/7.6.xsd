    <xsd:element name="be.pageRenderer">
        <xsd:annotation>
            <xsd:documentation>
                <![CDATA[View helper which allows you to create extbase based modules in the style of TYPO3 default modules.

= Examples =

<code title="All options">
<f:be.pageRenderer pageTitle="foo" loadExtJs="true" loadExtJsTheme="false" extJsAdapter="jQuery" enableExtJsDebug="true" loadJQuery="true" includeCssFiles="0: '{f:uri.resource(path:\'Css/Styles.css\')}'" includeJsFiles="0: '{f:uri.resource(path:\'JavaScript/Library1.js\')}', 1: '{f:uri.resource(path:\'JavaScript/Library2.js\')}'" addJsInlineLabels="{0: 'label1', 1: 'label2'}" />
</code>
<output>

Custom CSS file EXT:your_extension/Resources/Public/Css/styles.css and
JavaScript files EXT:your_extension/Resources/Public/JavaScript/Library1.js and EXT:your_extension/Resources/Public/JavaScript/Library2.js
will be loaded, plus ExtJS and jQuery and some inline labels for usage in JS code.
</output>
/]]>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType mixed="true">
            <xsd:sequence>
                <xsd:any minOccurs="0" maxOccurs="1" />
            </xsd:sequence>
            <xsd:attribute type="xsd:string" name="pageTitle" use="optional"
                           default="''">
                <xsd:annotation>
                    <xsd:documentation>
                        <![CDATA[title tag of the module. Not required by default, as BE modules are shown in a frame]]>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute type="xsd:boolean" name="loadExtJs" use="optional"
                           default="false">
                <xsd:annotation>
                    <xsd:documentation>
                        <![CDATA[specifies whether to load ExtJS library. Defaults to FALSE]]>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute type="xsd:boolean" name="loadExtJsTheme"
                           use="optional" default="true">
                <xsd:annotation>
                    <xsd:documentation>
                        <![CDATA[whether to load ExtJS "grey" theme. Defaults to FALSE]]>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute type="xsd:boolean" name="enableExtJsDebug"
                           use="optional" default="false">
                <xsd:annotation>
                    <xsd:documentation>
                        <![CDATA[if TRUE, debug version of ExtJS is loaded. Use this for development only]]>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute type="xsd:boolean" name="loadJQuery" use="optional"
                           default="false">
                <xsd:annotation>
                    <xsd:documentation>
                        <![CDATA[whether to load jQuery library. Defaults to FALSE]]>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute type="xsd:array" name="includeCssFiles"
                           use="optional" default="NULL">
                <xsd:annotation>
                    <xsd:documentation>
                        <![CDATA[List of custom CSS file to be loaded]]>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute type="xsd:array" name="includeJsFiles"
                           use="optional" default="NULL">
                <xsd:annotation>
                    <xsd:documentation>
                        <![CDATA[List of custom JavaScript file to be loaded]]>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute type="xsd:array" name="addJsInlineLabels"
                           use="optional" default="NULL">
                <xsd:annotation>
                    <xsd:documentation>
                        <![CDATA[Custom labels to add to JavaScript inline labels]]>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute type="xsd:array" name="includeRequireJsModules"
                           use="optional" default="NULL">
                <xsd:annotation>
                    <xsd:documentation>
                        <![CDATA[List of RequireJS modules to be loaded]]>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
